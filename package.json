{
  "name": "evex-theme",
  "displayName": "Evex Theme",
  "description": "Ultra cool vs code theme by Evex Developers",
  "icon": "logo.png",
  "publisher": "evex-dev",
  "author": "EdamAmex",
  "homepage": "https://github.com/evex-dev/theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evex-dev/theme"
  },
  "version": "1.1.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evex-dev/theme/issues"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "galleryBanner": {
    "color": "#111827",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "vs code",
    "theme",
    "dark",
    "light"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Evex Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Evex Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Evex Light+",
        "uiTheme": "vs",
        "path": "./themes/light+.json"
      },
      {
        "label": "Evex Light++",
        "uiTheme": "vs",
        "path": "./themes/light++.json"
      },
      {
        "label": "Evex Monochrome",
        "uiTheme": "vs-dark",
        "path": "./themes/monochrome.json"
      },
      {
        "label": "Evex Cyberpunk",
        "uiTheme": "vs-dark",
        "path": "./themes/cyberpunk.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.10.0"
  },
  "scripts": {
    "package": "mkdir -p build && vsce package -o ./build/vscode-evex-theme.vsix",
    "publish": "vsce publish"
  }
}
