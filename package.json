{
  "name": "pace-theme",
  "displayName": "Pace Theme",
  "description": "An elegant VS Code theme optimized for developers with a high pace",
  "icon": "icon.png",
  "publisher": "fabian-hiller",
  "author": "Fabian Hiller",
  "homepage": "https://github.com/fabian-hiller/vscode-pace-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabian-hiller/vscode-pace-theme"
  },
  "version": "0.2.1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabian-hiller/vscode-pace-theme/issues"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "galleryBanner": {
    "color": "#111827",
    "theme": "dark"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "vs code",
    "theme",
    "dark",
    "light"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Pace Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Pace Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Pace Light+",
        "uiTheme": "vs",
        "path": "./themes/light+.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.10.0"
  },
  "scripts": {
    "package": "mkdir -p build && vsce package -o ./build/vscode-pace-theme.vsix",
    "publish": "vsce publish"
  }
}
