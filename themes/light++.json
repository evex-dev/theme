    "editorIndentGuide.background": "#CBD5E1",
    "editorHoverWidget.background": "#F1F5F9",
    "editorHoverWidget.border": "#E2E8F0",
    "editorLineNumber.foreground": "#94A3B8",
    "editorRuler.foreground": "#CBD5E1",
    "editorSuggestWidget.background": "#F1F5F9",
    "editorSuggestWidget.border": "#E2E8F0",
    "editorSuggestWidget.selectedBackground": "#E2E8F0",
    "editorUnnecessaryCode.opacity": "#00000082",
    "editorWhitespace.foreground": "#CBD5E1",
    "editorWidget.background": "#F1F5F9",
    "gitDecoration.addedResourceForeground": "#065F46",
    "gitDecoration.modifiedResourceForeground": "#A16207",
    "gitDecoration.deletedResourceForeground": "#991B1B",
    "gitDecoration.renamedResourceForeground": "#065F46",
    "gitDecoration.stageModifiedResourceForeground": "#A16207",
    "gitDecoration.stageDeletedResourceForeground": "#991B1B",
    "gitDecoration.untrackedResourceForeground": "#065F46",
    "gitDecoration.ignoredResourceForeground": "#94A3B8",
    "gitDecoration.conflictingResourceForeground": "#B91C1C",
    "gitDecoration.submoduleResourceForeground": "#0369A1",
    "gitlens.gutterBackgroundColor": "#F1F5F9",
    "gitlens.gutterForegroundColor": "#64748B",
    "gitlens.trailingLineForegroundColor": "#94A3B8",
    "gitlens.lineHighlightBackgroundColor": "#BAE6FD40",
    "gitlens.lineHighlightOverviewRulerColor": "#BAE6FD",
    "input.background": "#E2E8F0",
    "input.foreground": "#334155",
    "input.placeholderForeground": "#64748B",
    "inputOption.activeBackground": "#BAE6FD",
    "inputOption.activeForeground": "#334155",
    "inputOption.hoverBackground": "#CBD5E1",
    "list.activeSelectionBackground": "#E2E8F0",
    "list.activeSelectionForeground": "#334155",
    "list.focusBackground": "#E2E8F0",
    "list.highlightForeground": "#334155",
    "list.hoverBackground": "#FFFFFF",
    "list.inactiveSelectionBackground": "#E2E8F0",
    "list.inactiveSelectionForeground": "#334155",
    "notifications.background": "#F1F5F9",
    "scrollbarSlider.activeBackground": "#94A3B880",
    "scrollbarSlider.background": "#E2E8F080",
    "scrollbarSlider.hoverBackground": "#CBD5E180",
    "sideBar.background": "#F1F5F9",
    "sideBar.foreground": "#475569",
    "sideBarSectionHeader.background": "#FFFFFF",
    "sideBarSectionHeader.foreground": "#475569",
    "sideBarTitle.foreground": "#475569",
    "statusBar.background": "#F1F5F9",
    "statusBar.foreground": "#64748B",
    "statusBarItem.hoverBackground": "#E2E8F0",
    "statusBar.noFolderBackground": "#F1F5F9",
    "statusBar.debuggingBackground": "#F1F5F9",
    "tab.activeBackground": "#E2E8F0",
    "tab.border": "#E2E8F0",
    "tab.inactiveBackground": "#F1F5F9",
    "terminal.foreground": "#64748B",
    "terminal.ansiBlack": "#E2E8F0",
    "terminal.ansiBlue": "#1D4ED8",
    "terminal.ansiGreen": "#047857",
    "terminal.ansiYellow": "#A16207",
    "terminal.ansiCyan": "#0F766E",
    "terminal.ansiMagenta": "#7E22CE",
    "terminal.ansiRed": "#B91C1C",
    "terminal.ansiWhite": "#334155",
    "terminal.ansiBrightBlack": "#94A3B8",
    "terminal.ansiBrightBlue": "#1D4ED8",
    "terminal.ansiBrightGreen": "#047857",
    "terminal.ansiBrightYellow": "#A16207",
    "terminal.ansiBrightCyan": "#0F766E",
    "terminal.ansiBrightMagenta": "#E879F9",
    "terminal.ansiBrightRed": "#DC2626",
    "terminal.ansiBrightWhite": "#334155",
    "textLink.foreground": "#1D4ED8",
    "titleBar.activeBackground": "#FFFFFF",
    "titleBar.activeForeground": "#64748B",
    "titleBar.inactiveBackground": "#FFFFFF",
    "titleBar.inactiveForeground": "#94A3B8",
    "window.activeBorder": "#1D4ED8"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "string.comment"],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#A16207"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#334155"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Package Import",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EA580C"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Builtin Functions",
      "scope": ["function.support.builtin", "function.support.core"],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Tag Content",
      "scope": ["text.html.derivative", "meta.jsx.children"],
      "settings": {
        "foreground": "#334155"
      }
    },
    {
      "name": "Tag Brackets",
      "scope": [
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "Tag Name",
      "scope": ["entity.name.tag", "entity.name.tag.class.js"],
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Tag Class and ID",
      "scope": ["entity.name.tag.class", "entity.name.tag.id"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.variable"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Json Property",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "StyleSheet Property name",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#64748B"
      }
    },
    {
      "name": "CSS: Pseudo Attribute Names",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "StyleSheet Property value",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "StyleSheet Variable",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "StyleSheet Variable String",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#A16207"
      }
    },
    {
      "name": "StyleSheet Unit",
      "scope": ["unit.css", "unit.scss", "unit.less", "unit.sass"],
      "settings": {
        "foreground": "#7E22CE"
      }
    },
    {
      "name": "StyleSheet Function",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "#7E22CE",
        "foreground": "#0F172A"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#0F766E",
        "foreground": "#0F172A"
      }
    },
    {
      "name": "JSON String",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "name": "diff.header",
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#A16207"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#0F766EA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#A16207"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#0369A1"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#B91C1C"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#047857"
      }
    },
    {
      "name": "Markup: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#7E22CE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Raw",
      "scope": ["markup.raw.inline.markdown", "markup.raw.block.markdown"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "name": "Markup: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#0F172A"
      }
    },
    {
      "name": "Object Literal",
      "scope": "meta.objectliteral",
      "settings": {
        "foreground": "#334155"
      }
    }
  ]
}
